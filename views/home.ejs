<%- include("partials/header") -%>
  <h1 class="cover-heading">Home</h1>
  <p class="lead"><%= content %></p>
  <% composeInputs.forEach(composeInput => { %>
    <h1 class="cover-heading"><%= composeInput.title %></h1>
    <% if (composeInput.post.length > 100) { %>
      <p class="lead"><%= composeInput.post.slice(0, 100) + " ... "%><a href="/posts/<%= composeInput.title %>">Read More</a></p>
    <% } else { %>
      <p class="lead"><%= composeInput.post %></p>
    <% } %>
  <% }) %>
<%- include("partials/footer") -%>
